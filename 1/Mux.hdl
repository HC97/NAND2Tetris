/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    //// Replace this comment with your code.
    Not(in= sel, out= seln);
    And(a= a, b= seln, out= oa);
    And(a= sel, b= b, out= ob);
    Or(a= oa, b= ob, out= out);
}