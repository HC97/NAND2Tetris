/**
 * 16-bit Or gate:
 * for i = 0, ..., 15:
 * out[i] = a[i] Or b[i] 
 */
CHIP Or16 {
    IN a[16], b[16];
    OUT out[16];

    PARTS:
    //// Replace this comment with your code.
    Not16(in= a, out= oa);
    Not16(in= b, out= ob);
    Nand16(a= oa, b= ob, out= out);
}